  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Command Line",
      "type": "python",
      "request": "launch",
      "stopOnEntry": false,
      "program": "${workspaceRoot}/src/glennscli/command_line.py",
      "console": "integratedTerminal",
      "args": [
        "tellmeajoke",
        "cat",
        "--numjokes",
        "3"
